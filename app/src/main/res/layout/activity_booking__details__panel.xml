<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Booking_Details_Panel"
    android:background="@drawable/dffff">





    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="63dp"
        android:fontFamily="serif"
        android:text="Booking Details"
        android:textAlignment="center"
        android:textColor="#FF8F00"
        android:textSize="30sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="128dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="179dp"
        android:fontFamily="serif"
        android:text="Starting From"
        android:textAlignment="center"
        android:textColor="#CDDC39"
        android:textSize="23dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="128dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView15"
        android:layout_centerVertical="true"
        android:fontFamily="serif"
        android:text="Going To"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="23dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView15"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="215dp"
        android:fontFamily="serif"
        android:text="Total Cost"
        android:textAlignment="center"
        android:textColor="#ffffff"
        android:textSize="23dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView15"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="100dp"
        android:fontFamily="serif"
        android:text="Travel Date"
        android:textAlignment="center"
        android:textColor="#CDDC39"
        android:textSize="23dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="100dp"
        android:layout_height="32dp"
        android:layout_alignBaseline="@+id/textView15"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="44dp"
        android:background="@drawable/edit1"
        android:textAlignment="center"
        android:textStyle="bold" />


    <TextView
        android:id="@+id/textView24"
        android:layout_width="100dp"
        android:layout_height="33dp"
        android:layout_alignStart="@+id/textView16"
        android:layout_centerVertical="true"
        android:background="@drawable/edit1"
        android:textAlignment="center"
        android:textStyle="bold" />


    <TextView
        android:id="@+id/textView26"
        android:layout_width="100dp"
        android:layout_height="32dp"
        android:layout_alignStart="@+id/textView16"
        android:layout_alignTop="@+id/textView25"
        android:background="@drawable/edit1"
        android:textAlignment="center"
        android:textStyle="bold" />


    <TextView
        android:id="@+id/textView28"
        android:layout_width="100dp"
        android:layout_height="32dp"
        android:layout_alignStart="@+id/textView16"
        android:layout_alignTop="@+id/textView27"
        android:background="@drawable/edit1"
        android:textAlignment="center"
        android:textStyle="bold" />

</RelativeLayout>